<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Price</th>

      <th></th>
    </tr>
  </thead>
  <tbody id="products">
    <%= for product <- @products do %>
      <tr id={"product-#{product.id}"}>
        <td><%= product.name %></td>
        <td><%= product.price %></td>

        
      </tr>
    <% end %>
  </tbody>
</table>